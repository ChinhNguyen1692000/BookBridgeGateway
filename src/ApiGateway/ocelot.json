{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgeuserservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/auth/{everything}",
      "UpstreamHttpMethod": [
        "Post",
        "Get",
        "Put",
        "Delete"
      ],
      "Priority": 1,
      "Key": "UserServiceAuth",
      "SwaggerKey": "auth"
    },
    {
      "DownstreamPathTemplate": "/active",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgebookservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/books/active",
      "UpstreamHttpMethod": [
        "Get"
      ],
      "Priority": 2,
      "Key": "BookServiceActive",
      "SwaggerKey": "books"
    },
    {

      "DownstreamPathTemplate": "/unactive",
      "UpstreamPathTemplate": "/gateway/books/unactive",
      "Key": "BookServiceUnactive",
      "SwaggerKey": "books"
    },
    {
      "DownstreamPathTemplate": "/api/books/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgebookservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/books/{everything}",
      "UpstreamHttpMethod": [
        "Post",
        "Get",
        "Put",
        "Delete"
      ],
      "Priority": 2,
      "Key": "BookService",
      "SwaggerKey": "books"
    },
    {
      "DownstreamPathTemplate": "/api/bookstores/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgebookstoreservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/bookstores/{everything}",
      "UpstreamHttpMethod": [
        "Post",
        "Get",
        "Put",
        "Delete"
      ],
      "Priority": 3,
      "Key": "BookstoreService",
      "SwaggerKey": "bookstores"
    },
    {
      "DownstreamPathTemplate": "/api/orders/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgeorderservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/orders/{everything}",
      "UpstreamHttpMethod": [
        "Post",
        "Get",
        "Put",
        "Delete"
      ],
      "Priority": 4,
      "Key": "OrderService",
      "SwaggerKey": "orders"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Auth Service",
          "Version": "v1",
          "Url": "https://bookbridgeuserservice.onrender.com/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "books",
      "Config": [
        {
          "Name": "Book Service",
          "Version": "v1",
          "Url": "https://bookbridgebookservice.onrender.com/swagger/v1/swagger.json",
          "TakeServersNameAndPortFromConfig": true
        }
      ]
    },
    {
      "Key": "bookstores",
      "Config": [
        {
          "Name": "Bookstore Service",
          "Version": "v1",
          "Url": "https://bookbridgebookstoreservice.onrender.com/swagger/v1/swagger.json",
          "TakeServersNameAndPortFromConfig": true
        }
      ]
    },
    {
      "Key": "orders",
      "Config": [
        {
          "Name": "Order Service",
          "Version": "v1",
          "Url": "https://bookbridgeorderservice.onrender.com/swagger/v1/swagger.json",
          "TakeServersNameAndPortFromConfig": true
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://bookbridgegateway.onrender.com"
  }
}