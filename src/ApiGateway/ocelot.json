{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgeuserservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/auth/{everything}",
      "UpstreamHttpMethod": [ "Post", "Get", "Put", "Delete" ],
      "Priority": 1,
      "Key": "UserServiceAuth"
    },
    {
      "DownstreamPathTemplate": "/api/books/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgebookservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/books/{everything}",
      "UpstreamHttpMethod": [ "Post", "Get", "Put", "Delete" ],
      "Priority": 2,
      "Key": "BookService"
    },
    {
      "DownstreamPathTemplate": "/api/bookstores/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgebookstoreservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/bookstores/{everything}",
      "UpstreamHttpMethod": [ "Post", "Get", "Put", "Delete" ],
      "Priority": 3,
      "Key": "BookstoreService"
    },
    {
      "DownstreamPathTemplate": "/api/orders/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookbridgeorderservice.onrender.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/gateway/orders/{everything}",
      "UpstreamHttpMethod": [ "Post", "Get", "Put", "Delete" ],
      "Priority": 4,
      "Key": "OrderService"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://bookbridgegateway.onrender.com"
  }
}
